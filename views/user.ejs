<div class="container">

<div id="input-wrapper">
  <form action="/users" method="POST" class="user-form">
    <div class="revenues">
      <div class="title-value-revenue">
        <input type="text" placeholder="revenue type" id="revenue">
        <input type="number" placeholder="value" id="revenue-value">
        <i class="fas fa-plus-circle add-revenue"></i>
      </div>
      <!-- <div class="title/value">
        <input type="text" placeholder="revenue type">
